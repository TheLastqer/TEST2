    local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
    local Window = Library:NewWindow("Script")
    
    local Tab = Window:NewSection("Credits: FxZ Hub")

    Tab:CreateToggle("AUTO UPGRADE", function(no)
    _G.GetWins = no
    while _G.GetWins== true do
        wait() 
        
game:GetService("ReplicatedStorage").Events.ClickMoney:FireServer()

local args = {
    [1] = 2,
    [2] = true
}

game:GetService("ReplicatedStorage").Events.Upgrade:FireServer(unpack(args))

